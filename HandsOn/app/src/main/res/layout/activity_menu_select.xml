<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuSelect">

    <Button
        android:id="@+id/last_result_button"
        android:background="@drawable/button_lightblue"
        android:fontFamily="@font/nanumsquare_acb"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:textSize="20dp"
        android:text="지난 설문 조회"
        app:layout_constraintTop_toTopOf="@+id/mbti_button"
        app:layout_constraintEnd_toStartOf="@+id/mbti_button"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="30dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/mbti_button"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:background="@drawable/button_lightblue"
        android:fontFamily="@font/nanumsquare_acb"
        android:text="MBTI 설정"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/last_result_button"
        tools:ignore="MissingConstraints"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="100dp"/>

    <Button
        android:id="@+id/test_start_button"
        android:background="@drawable/button_lightblue"
        android:layout_width="130dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:layout_height="100dp"
        android:textSize="20dp"
        android:text="설문 진행"
        app:layout_constraintLeft_toLeftOf="@+id/last_result_button"
        app:layout_constraintTop_toBottomOf="@+id/last_result_button"
        android:layout_marginTop="30dp"
        tools:ignore="MissingConstraints"/>

    <ImageView
        android:id="@+id/logo"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/logo_square"
        app:layout_constraintStart_toEndOf="@+id/test_start_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/test_start_button"
        android:layout_marginRight="30dp" />


</androidx.constraintlayout.widget.ConstraintLayout>