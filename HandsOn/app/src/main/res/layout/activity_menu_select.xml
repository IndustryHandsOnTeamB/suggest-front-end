<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuSelect">

    <Button
        android:id="@+id/last_result_button"
        android:background="@drawable/button_lightblue"
        android:fontFamily="@font/nanumsquare_acb"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:textSize="20dp"
        android:text="지난 설문 조회"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/mbti_button"
        android:layout_marginBottom="50dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/mbti_button"
        android:background="@drawable/button_lightblue"
        android:fontFamily="@font/nanumsquare_acb"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:textSize="20dp"
        android:text="MBTI 설정"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/test_start_button"
        android:background="@drawable/button_lightblue"
        android:layout_width="200dp"
        android:fontFamily="@font/nanumsquare_acb"
        android:layout_height="100dp"
        android:textSize="20dp"
        android:text="설문 진행"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mbti_button"
        android:layout_marginTop="50dp"
        tools:ignore="MissingConstraints"/>


</androidx.constraintlayout.widget.ConstraintLayout>